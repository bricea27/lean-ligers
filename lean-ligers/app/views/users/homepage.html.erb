<p id="notice"><%= notice %></p>

<section id="user-homepage">
<div class="container">
	<div class="row">
		<div class="col-md-6">
			<h1> Hello, <%= current_user.first_name %></h1>
			<%= link_to 'Edit Profile', edit_mentee_path(current_user), class: "btn btn-default" %>
		</div>
	</div>
	<% if current_user.type == 'Mentor' %>
		<%= render 'users/mentors/home' %>
	<% else %>
		<%= render 'users/mentees/home' %>
	<% end %>

</div>
</section>
